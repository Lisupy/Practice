[32m+[m[32m(define (my-equal? a b)[m
[32m+[m[32m  (cond ((null? a) (null? b))[m
[32m+[m[32m        ((eq? (car a) (car b)) (my-equal? (cdr a) (cdr b)))[m
[32m+[m[32m        (else false)))[m
[32m+[m[41m                   [m
